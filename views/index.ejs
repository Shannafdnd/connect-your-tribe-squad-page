<%- include('./partials/head') %>

<h2>Students</h2>
<ul>
  <% persons.forEach(person => { %>
    <li><a href="./person/<%= person.id %>"><%= person.name %></a>, 
      squad_id <%= person.squad_id %>
      <img src="<%= person.avatar %>" alt="avatar">
    </li>
  <% }) %>
</ul>

<%- include('./partials/foot') %>